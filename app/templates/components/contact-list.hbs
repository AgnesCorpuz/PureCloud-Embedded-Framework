<div style="padding-left: 50px; padding-top: 10px">
    <h1 class="w3-text-deep-orange">List of Contacts</h1>
    <div id="myBody">
        <table>
            <thead>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Company</th>
                <th>Phone</th>
                <th>Email</th>
            </thead>
            <tbody>                
                {{#each contactList as |contact|}}
                    <tr>
                        <td>{{contact.FirstName}}</td>
                        <td>{{contact.LastName}}</td>
                        <td>{{contact.Account}}</td>
                        <td  {{action "clickToDial" contact.Phone}}>{{contact.Phone}}</td>
                        <td>{{contact.Email}}</td>
                    </tr>                        
                {{/each}}
            </tbody>
        </table>
    </div>
</div>

{{yield}}